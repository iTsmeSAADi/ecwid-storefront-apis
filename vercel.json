{
    "version": 2,
    "functions": {
      "index.js": {
        "maxDuration": 15,
        "memory": 1024,
        "runtime": "nodejs18.x"
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "env": {
      "AWS_REGION": "us-east-1"
    }
  }
  