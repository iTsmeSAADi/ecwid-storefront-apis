{
    "version": 2,
    "functions": {
      "index.js": {
        "maxDuration": 15,
        "memory": 1024
      }
    },
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ],
    "env": {
      "AWS_REGION": "us-east-1"
    }
  }
  